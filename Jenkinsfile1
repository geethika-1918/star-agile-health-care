pipeline {

    agent any

    stages {

        stage('Checkout') {

            steps {

                echo "checked out"
           sleep(time: 974, unit: 'MILLISECONDS')

            }

        }
 
        stage('Build') {

            steps {

                echo "code compiled"
sleep(time: 63, unit: 'MILLISECONDS')

            }

        }
 
        stage('Test') {

            steps {

                echo "maven test"
sleep(time: 142, unit: 'MILLISECONDS')

            }

        }
 
        stage('Package') {

            steps {

                echo "packaging your code"
sleep(time: 127, unit: 'MILLISECONDS')
            }

        }
 
        stage('Provision Test Infrastructure') {

            steps {

                echo "provisioning test infrastructure"
sleep(time: 163, unit: 'MILLISECONDS')

            }

        }
 
        stage('Configure Servers') {

            steps {

                echo "configuring servers"
sleep(time: 149, unit: 'MILLISECONDS')

            }

        }
 
        stage('Docker Build') {

            steps {

                echo "built the docker image"
sleep(time: 58, unit: 'MILLISECONDS')

            }

        }
 
        stage('Deploy to Test') {

            steps {

                echo "deployed the test phase"
sleep(time: 128, unit: 'MILLISECONDS')

            }

        }
 
        stage('Deploy to Production') {

            steps { // Added the "steps" block here

                echo "deployed the production phase"
sleep(time: 187, unit: 'MILLISECONDS')

            }

        }
 
        stage('Setup Monitoring') {

            steps {

                echo "monitoring"
sleep(time: 83, unit: 'MILLISECONDS')

            }

        }

    }

}
 
